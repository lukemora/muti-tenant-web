{
  "name": "@multi-tenant/web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "pnpm --filter @multi-tenant/business-platform dev",
    "dev:business": "pnpm --filter @multi-tenant/business-platform dev",
    "dev:admin": "pnpm --filter @multi-tenant/admin-platform dev",
    "build": "pnpm --filter @multi-tenant/business-platform build",
    "build:business": "pnpm --filter @multi-tenant/business-platform build",
    "build:admin": "pnpm --filter @multi-tenant/admin-platform build",
    "build:all": "pnpm -r build",
    "clean": "pnpm -r clean",
    "reinstall": "rimraf node_modules pnpm-lock.yaml && pnpm install",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint:fix",
    "format": "prettier --write \"packages/**/*.{js,jsx,ts,tsx,vue,json,css,md}\"",
    "format:check": "prettier --check \"packages/**/*.{js,jsx,ts,tsx,vue,json,css,md}\"",
    "type-check": "pnpm -r type-check",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@element-plus/icons-vue": "latest",
    "@unocss/vite": "^66.5.0",
    "@vueuse/core": "latest",
    "axios": "latest",
    "element-plus": "latest",
    "lodash": "latest",
    "moment": "latest",
    "pinia": "latest",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@eslint/js": "latest",
    "@iconify-json/carbon": "^1.2.13",
    "@iconify-json/ep": "^1.2.3",
    "@iconify-json/ri": "^1.2.5",
    "@types/lodash": "latest",
    "@types/moment": "latest",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "@unocss/preset-icons": "^66.4.2",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "latest",
    "autoprefixer": "^10.4.21",
    "browserslist": "^4.26.2",
    "buffer": "^6.0.3",
    "cssnano": "^7.1.1",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unicorn": "latest",
    "eslint-plugin-vue": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "postcss": "^8.4.32",
    "prettier": "latest",
    "process": "^0.11.10",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "^1.90.0",
    "sass-loader": "^16.0.5",
    "typescript": "latest",
    "unocss": "^66.4.2",
    "unplugin-auto-import": "^20.1.0",
    "unplugin-vue-components": "^29.1.0",
    "vite": "^7.0.4",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vue-eslint-parser": "latest",
    "vue-tsc": "^2.2.12"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "browserslist": ["Chrome >= 118", "Firefox >= 118", "Safari >= 17", "Edge >= 118"],
  "lint-staged": {
    "packages/**/*.{js,jsx,ts,tsx,vue}": ["eslint --fix", "prettier --write"],
    "packages/**/*.{json,css,md}": ["prettier --write"]
  }
}
